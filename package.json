{
	"name": "Pink-Palace-Gardening-Frontend",
	"version": "0.0.1",
	"private": true,
	"engines": {
		"node": ">=6.5",
		"npm": ">=3.10"
	},
	"description": "Garden Calendar for tracking Pink Palace gardening schedule",
	"main": "index.js",
	"author": "Kyle Combeer",
	"license": "MIT",
	"dependencies": {
		"babel-cli": "^6.24.1",
		"babel-core": "^6.25.0",
		"babel-loader": "^7.1.1",
		"html-webpack-plugin": "^2.29.0",
		"moment": "^2.18.1",
		"react": "^15.6.1",
		"react-big-calendar": "^0.14.4",
		"react-dom": "^15.6.1",
		"react-router-dom": "^4.1.1",
		"winston": "^2.3.1"
	},
	"devDependencies": {
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"css-loader": "^0.28.4",
		"style-loader": "^0.18.2",
		"webpack": "^3.0.0"
	},
	"scripts": {
		"lint:js": "eslint public tools",
		"lint:css": "stylelint \"public/**/*.{css,less,scss,sss}\"",
		"lint": "yarn run lint:js && yarn run lint:css",
		"start": "yarn build && yarn buildServer && yarn bundle && yarn serve",
		"build": "babel public -d build --presets=es2015,stage-2,react",
		"buildServer": "babel index.js -d . --presets=es2015,stage-2,react",
		"bundle": "webpack",
		"serve": "nodemon index.js --exec babel-node --presets react,es2015"
	}
}
